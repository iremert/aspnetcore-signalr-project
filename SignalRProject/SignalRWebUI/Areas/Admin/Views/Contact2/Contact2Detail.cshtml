@model ResultContact2Dto
@{
    ViewData["Title"] = "Contact2Detail";
    Layout = "~/Views/Layout/AdminLayout.cshtml";
}


<div class="content">
    <div class="container-fluid">
        <h4 class="page-title">İletişim Sol Kısım İşlemleri</h4>
        <div class="row">
            <div class="col-md-12">
                <form method="post" class="forms-sample" enctype="multipart/form-data">
                    <div class="card">
                        <div class="card-header">
                            <div class="card-title">İletişim Sol Kısım Detay</div>
                        </div>
                        <div class="card-body">
                            <div class="form-group">
                                <label for="Adres">Adres</label>
                                <input type="text" class="form-control" asp-for="Address" readonly id="Adres">

                            </div>
                            <div class="form-group">
                                <label for="Phone">Telefon</label>
                                <input type="text" class="form-control" asp-for="Phone" readonly id="Phone">
                            </div>
                            <div class="form-group">
                                <label for="Email">Mail</label>
                                <input type="email" class="form-control" asp-for="Email" readonly id="Email">
                            </div>

                            <div class="form-group">
                                <label for="Map">Harita</label>
                                <textarea rows="3" type="text" class="form-control" asp-for="Map" readonly id="Map"></textarea>
                            </div>

                    </div>
                </form>
            </div>
        </div>
    </div>
</div>


