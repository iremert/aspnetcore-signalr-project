@model List<ResultContact2Dto>

@foreach(var item in Model)
{
    <section class="w3l-contact" id="contact">
        <div class="text-center mb-3 pb-3">
            <div class="container">
                <h3 class="text-primary text-uppercase" style="letter-spacing: 5px;">Bize Ulaşın</h3>
                <div class="contact-map inner-sec">
                    <iframe src="@item.Map" width="600" height="450" style="border:3;width:100%; height:400px;" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
                </div>
            </div>
        </div>

    </section>
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <div class="col-md-4">
        <div class="row mb-5">
            <div class="col-md-12">
                <div style="background-color:lightsteelblue;border-radius:8px;">
                    <br />
                    <div style="color:mediumblue;text-align:center;">
                        <span class="fa fa-map" style="font-size:30px;"></span>
                    </div>
                    <p><span>Adres:</span> @item.Address</p>
                    <br />
                </div>
                <br />
                <br />
            </div>
            <div class="col-md-12">
                <div style="background-color:lightsteelblue;border-radius:8px;">
                    <br />
                    <div style="color:mediumblue;text-align:center;">
                        <span class="fa fa-mobile-phone" style="font-size:50px;"></span>
                    </div>
                    <p><span>Telefon:</span>@item.Phone</p>
                    <br />
                </div>
                <br />
                <br />
            </div>
            <div class="col-md-12">
                <div style="background-color:lightsteelblue;border-radius:8px;">
                    <br />
                    <div style="color:mediumblue;text-align:center;">
                        <span class="fa fa-envelope" style="font-size:30px;"></span>
                    </div>
                    <p><span>Mail:</span> @item.Email</p>
                    <br />
                </div>
            </div>
        </div>
    </div>

}
   